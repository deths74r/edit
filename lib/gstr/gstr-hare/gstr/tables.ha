// Binary search for codepoint in ranges
export fn range_contains(ranges: []unicode_range, cp: u32) bool = {
	let low: size = 0;
	let high: size = len(ranges);
	
	for (low < high) {
		const mid = low + (high - low) / 2;
		if (cp < ranges[mid].start) {
			high = mid;
		} else if (cp > ranges[mid].end) {
			low = mid + 1;
		} else {
			return true;
		};
	};
	return false;
};
