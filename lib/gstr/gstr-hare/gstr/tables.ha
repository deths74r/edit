// Unicode property tables for grapheme cluster segmentation (Unicode 17.0)

// Grapheme Cluster Break property values from UAX #29
export type gcb = enum u8 {
	OTHER = 0,
	CR,
	LF,
	CONTROL,
	EXTEND,
	ZWJ,
	REGIONAL_INDICATOR,
	PREPEND,
	SPACING_MARK,
	L,
	V,
	T,
	LV,
	LVT,
};

// Range of Unicode codepoints
export type unicode_range = struct {
	start: u32,
	end: u32,
};

// GCB property range with associated property
export type gcb_range = struct {
	start: u32,
	end: u32,
	prop: gcb,
};

// Hangul constants for algorithmic LV/LVT detection
export def HANGUL_SBASE: u32 = 0xAC00;
export def HANGUL_SEND: u32 = 0xD7A3;
export def HANGUL_TCOUNT: u32 = 28;
export def GRAPHEME_MAX_BACKTRACK: size = 128;

